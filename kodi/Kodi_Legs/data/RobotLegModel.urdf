<!-- Add weight on the top of Torso -->
<link name="weight">
	<visual>
		<origin rpy="0 0 0" xyz="0.02 0 0"/>
		<geometry>
			<cylinder radius="0.045" length="0.03" />
		</geometry>
		<material name="red">
			<color rgba="1 0 0 1"/>
		</material>
	</visual>
	<collision>
		<origin rpy="0 0 0" xyz="0.02 0 0"/>
