<?xml version="1.0"?>
<robot name="TestBot">

<!-- Base Link: Torso -->
<link name ="Body">
    <visual>
		<geometry>
			<mesh filename="Kobot/robotBody.obj"  scale="0.01 0.01 0.01" />
		</geometry>	 
    </visual>
    <collision>	
      <geometry>
        <mesh filename="Kobot/robotBody.obj"  scale="0.01 0.01 0.01" />
      </geometry>
    </collision>
	<inertial>
		<mass value="0.4"/>
		<inertia ixx="0.001148" ixy="0.0" ixz="0.0" iyy="0.0008363" iyz="0.0" izz="0.001148"/>
	</inertial>
</link>

